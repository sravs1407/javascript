<!DOCTYPE html>
<html>
<body>
<button onclick="big()">biggest</button>
<p1 id="res"></p1>
<script>
function big(){
a=prompt("enter first number");
b=prompt("enter second number");
c=prompt("enter third number");
if (a>b && a>c){
document.getElementById("res").innerHTML=a+" is big";
}else if(b>a && b>c){
document.getElementById("res").innerHTML=b+" is big";
}else{
document.getElementById("res").innerHTML=c+" is big";
}
}
</script>
</body>
</html>
